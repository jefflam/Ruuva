/* Specific Styling */

body { 
	padding-top:40px; 
	background-color: #fafafa; 
}
.thumbnail { background-color: #fff; }
.thumbnails > li > a { color:#222; }
.thumbnails > li > a:hover { text-decoration:none; }

/* Breadcrumb button styling */
.breadcrumb-btn { margin-top: -5px; }
.breadcrumb-btn-left { margin-left: 5px; }

/* Sticky Footer */
* {	margin: 0; }
html, body { height: 100%;}
.wrapper {
	min-height: 95%;
	height: auto !important;
	height: 100%;
	margin: 0 auto -4em;
}
.footer, .push { height: 4em; clear:both;}
footer { 
	margin:0 auto; 
	border-top:1px solid #ccc;
	margin-top:20px;
	padding-top:10px;
}
