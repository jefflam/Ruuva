<div class="container headline page-header">
	<h1><%= @title %></h1>
</div>			
<!-- Center Column, Main -->
<% unless @users.empty? %>
<ul class="thumbnails">
	<% @users.each do |user| %>
	<li>	
		<%= link_to(user, class: "thumbnail") do %>
			<%= gravatar_for(user, size:190) %>
			<div class="caption">
				<h5>
					<%= user.name %>
				</h5>
				<p>
					Clothes, Male
				</p>
			</div>
		<% end %>			
	</li>
	<% end %>	
</ul>
<% end %>
